<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Search by Answer</title>
  <link rel="stylesheet" href="../css/styles.css">
</head>
<body>
	<header>
    <div class="container">
      <h1>Students Feedback System</h1>
    </div>
  </header>

  <div class="container">
    <h3>Search by Answer</h3>
    <a href="/" class="button">Home</a><br>
  </div>
  <br>

  <div class="container">
    <table border="1">
      <tr>
        <td>Location</td>
        <td>Date</td>
        <td>Time</td>
        <td>Answer</td>
      </tr>
      <% for(var i=0; i < ans.length; i++) { %>
      <tr>
        <td><%- ans[i].location %></td>
        <td><%- ans[i].date %></td>
        <td><%- ans[i].time %></td>
        <td><%- ans[i].answer %></td>
      </tr>
      <% } %>
    </table>
  </div>

<br>
<footer></footer>

</body>
</html> 